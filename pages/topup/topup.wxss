/* pages/topup/topup.wxss */
.card_activity{
  display:-webkit-box;
  height: 80rpx;
  background: url(data:image/png;base64,);
  background-size:100% 80rpx;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
}
.card_activity .left{
  -webkit-box-flex:1;
  display:block;
  opacity: 0.88;
  font-size: 28rpx;
  color: #4F5EFF;
  vertical-align: middle;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}

.card_activity .right{
  display: block;
  vertical-align: middle;
}
.card_activity .right .card_activity_img{
  display: inline-block;
  margin-left: 10rpx;
  width: 36rpx;
  height: 36rpx;
}
.card_activity .right .card_right{
  display: flex;
  justify-content:center;
  align-items:center;
}
.card_activity .right .card_right_img{
  display: inline-block;
  margin-left: 10rpx;
  vertical-align: middle;
  width: 12rpx;
  height: 20rpx;
}
.listItem_desc{
  display: inline-block;
  padding-left: 20rpx;
  font-size: 24rpx;
  color: #BBBBBB;
  letter-spacing: 1.71rpx;
}

.intergral_input{
  display: inline-block;
  vertical-align:middle;
  width: 284rpx;  
  height: 58rpx;
  border: 1rpx solid #4F5EFF;
  border-radius: 12rpx;
  color: #4F5EFF;
}
.card_title{height: 78rpx;border-bottom: 2rpx solid #E4E4E4;background:#ffffff;}
.card_title .left{display: inline-block;padding:0 25rpx 0 30rpx;vertical-align: middle;}
.card_title .left .card_title_img{width: 32rpx;height: 25rpx;}
.card_title .right{line-height: 78rpx;font-size: 32rpx;color: #535353;}
.card_wrap{padding:30rpx;clear:both;background:#ffffff;}
.card_list{position:relative;display:inline-block;width:33.33%;vertical-align: top;}
.card_list view,.card_list input{
  text-align:center;margin-right:15rpx;margin-bottom:15rpx;font-size:34rpx;
  height:148rpx;line-height:148rpx;color:#333333;
  border:2rpx solid #D6DAFF;border-radius:10rpx;background:#ffffff;
}
.card_list:nth-child(3n+0) view,.card_list:nth-child(3n+0) input{margin-right:0;}
.card_list view.on{color:#4F5EFF;border-color:#4F5EFF;}
.card_list input.on{color:#4F5EFF;border-color:#4F5EFF;}
.card_list .check_on{position: absolute;top: 0rpx;right: 0rpx;width: 44rpx;height: 44rpx;
  background: url(data:image/png;base64,);
  background-size: 44rpx 44rpx;
  border:none;
}
.card_coupon{margin:20rpx 0;}
.topup_check{line-height: 28rpx;font-size:28rpx;margin-top:30rpx;padding:0 30rpx;}

.swift_checkbox{display:inline-block;border:1px solid #FAA122;border-radius: 50%; vertical-align:middle; width:24rpx;height:24rpx;overflow: hidden;font-size: 0rpx;}
.swift_checkbox .swift_checkbox_img{width:24rpx;height:24rpx;}
.swift_checkbox_txt{font-size: 20rpx;color: #999999;display:inline-block;margin-left:7rpx;vertical-align:middle;}

.switch_wrap{
  vertical-align: middle;
  width: 89rpx;
  height: 45rpx;
}
.switch_wrap .switch_img{
  width: 89rpx;
  height: 45rpx;
}
